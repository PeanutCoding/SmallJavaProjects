<?xml version="1.0" encoding="utf-8"?>
<!-- an alphabet with it's morse representation for each character
      I chose xml to have clear distinction and it is easy to append more
      characters -->
<Morse>
    <letters>
        <letter>
            <token id="a" morse=".-">A</token>
        </letter>
        <letter>
            <token id="b" morse="-...">B</token>
        </letter>
        <letter>
            <token id="c" morse="-.-.">C</token>
        </letter>
        <letter>
            <token id="d" morse="-..">D</token>
        </letter>
        <letter>
            <token id="e" morse=".">E</token>
        </letter>
        <letter>
            <token id="f" morse="..-.">F</token>
        </letter>
        <letter>
            <token id="g" morse="--.">G</token>
        </letter>
        <letter>
            <token id="h" morse="....">H</token>
        </letter>
        <letter>
            <token id="i" morse="..">I</token>
        </letter>
        <letter>
            <token id="j" morse=".---">J</token>
        </letter>
        <letter>
            <token id="k" morse="-.-">K</token>
        </letter>
        <letter>
            <token id="l" morse=".-..">L</token>
        </letter>
        <letter>
            <token id="m" morse="--">M</token>
        </letter>
        <letter>
            <token id="n" morse="-.">N</token>
        </letter>
        <letter>
            <token id="o" morse="---">O</token>
        </letter>
        <letter>
            <token id="p" morse=".--.">P</token>
        </letter>
        <letter>
            <token id="q" morse="--.-">Q</token>
        </letter>
        <letter>
            <token id="r" morse=".-.">R</token>
        </letter>
        <letter>
            <token id="s" morse="...">S</token>
        </letter>
        <letter>
            <token id="t" morse="-">T</token>
        </letter>
        <letter>
            <token id="u" morse="..-">U</token>
        </letter>
        <letter>
            <token id="v" morse="...-">V</token>
        </letter>
        <letter>
            <token id="w" morse=".--">W</token>
        </letter>
        <letter>
            <token id="x" morse="-..-">X</token>
        </letter>
        <letter>
            <token id="y" morse="-.--">Y</token>
        </letter>
        <letter>
            <token id="z" morse="--..">Z</token>
        </letter>
        <letter>
            <token id="." morse="E">.</token>
        </letter>
        <letter>
            <token id="?" morse="..--..">?</token>
        </letter>
        <letter>
            <token id=" " morse="/">backslash</token>
        </letter>
        <letter>
            <token id="!" morse="-.-.--">!</token>
        </letter>
    </letters>
    <numbers>
        <number>
            <token id="1" morse=".----">1</token>
        </number>
        <number>
            <token id="2" morse="..---">2</token>
        </number>
        <number>
            <token id="3" morse="...--">3</token>
        </number>
        <number>
            <token id="4" morse="....-">4</token>
        </number>
        <number>
            <token id="5" morse=".....">5</token>
        </number>
        <number>
            <token id="6" morse="-....">6</token>
        </number>
        <number>
            <token id="7" morse="--...">7</token>
        </number>
        <number>
            <token id="8" morse="---..">8</token>
        </number>
        <number>
            <token id="9" morse="----.">9</token>
        </number>
        <number>
            <token id="0" morse="-----">0</token>
        </number>
    </numbers>
</Morse>